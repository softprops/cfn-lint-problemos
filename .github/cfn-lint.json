{
    "problemMatcher": [
        {
            "owner": "cfn-lint",
            "pattern": [
                {
                    "regexp": "^(E|W|I)(\\d{4})\\s(.+)$",
                    "severity": 1,
                    "code": 2,
                    "message": 3
                },
                {
                    "regexp": "^(\\S+):(\\d+):(\\d+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                }
            ]
        }
    ]
}